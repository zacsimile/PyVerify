program gcd
    pre x > 0
    pre y > 0
    post x % d = 0
    post y % d = 0
    post forall n, ((x % n = 0 && y % n = 0) ==> n <= d)
is
    d := 1;
    i := 1;
    while i <= x && i <= y
        inv 1 <= i
        inv i <= x + 1
        inv i <= y + 1
        inv x % d = 0
        inv y % d = 0
        inv forall j, ((x % j = 0 && y % j = 0 && j < i) ==> j <= d)
    do
        if x % i = 0 && y % i = 0 then
            d := i;
        end
        i := i + 1;
    end
end